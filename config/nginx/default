                add_header 'Cache-Control' 'private';

        }

        location / {
            rewrite            ^/accounts/saml/login/ /accounts/fedict/login/ last;
            proxy_pass         http://unix:/run/authentic2-multitenant/authentic2-multitenant.sock;
            proxy_set_header   Host $http_host;
